<%- headers = ['Name:', 'Email:', 'Team id:', 'Start Date:', 'End Date:'] -%>

<%= CSV.generate_line headers %>

<%- @missing.each do |f| -%>
    <%- f[1].each do |user_data| -%>
         <%= CSV.generate_line([user_data.full_name, user_data.email, user_data.teams.first.team_name, @start_date, @end_date]) -%>
    <%- end -%>
<%- end -%>